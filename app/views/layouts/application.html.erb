<!DOCTYPE html>
<html>
<head>
	<title>Depot</title>
	<%= stylesheet_link_tag :all %>
	<%= javascript_include_tag :defaults %>
	<%= csrf_meta_tag %>
</head>
<body id="store">
	<div id="banner">
		<%= image_tag("logo.png") %>
		<%= @page_title || "Pragmatic Bookshelf" %>
	</div>
	<div id="columns">
		<div id="side">
			<%= link_to "Home",'#' %><br />
			<%= link_to "Question",'#' %><br />
			<%= link_to "news",'#' %><br />
			<%= link_to "Contact",'#' %><br />
		</div>
	</div>
	<div id="main">
		<%= yield %>
	</div>
</body>
</html>
